#include <iostream>
#include <math.h>
#include <string>
#include <stdio.h>

using namespace std;

// функция для печати таблицы Пифагора
void getPiphagor(){
    for (int i = 1; i < 11; i++){
        for (int j = 1; j < 11; j++){
            cout << i*j << "\t";
        }
        cout << endl;
    }
}

int main()
{    
    cout << "Выведем таблицу Пифагора" << endl;
    getPiphagor();
    
    return 0;
}
