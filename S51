#include <iostream>
#include <math.h>
#include <string>
#include <stdio.h>

using namespace std;

// Функция реверса строки
void reverse(string s) {
    int n = s.length();
    for (int i = 0; i < n / 2; i++) {
        swap(s[i], s[n - i - 1]);
    }
    cout << s << endl;
}

int main()
{    
    cout << "line -> ";
    reverse("line");
    cout << "Gena Tornoev -> ";
    reverse("Gena Tornoev");
    cout << endl;
     
    return 0;
}
